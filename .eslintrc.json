{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console" :"off",
        "linebreak-style" :"off",
        "no-extraneous-dependencies" :"off",
        "no-unused-vars":"off",
        "import/no-extraneous-dependencies": "off"
    }
}
